<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" minHeight="122.0" prefWidth="501.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p2pApp.p2pUi.controller.AlertController">
  <children>
    <AnchorPane fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="titleLabel" layoutX="21.0" layoutY="9.0" prefHeight="39.0" prefWidth="440.0" text="Label">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label fx:id="infoLabel" layoutX="27.0" layoutY="47.0" prefHeight="39.0" prefWidth="432.0" text="Label" wrapText="true" />
            <Button fx:id="cancelBtn" layoutX="440.0" layoutY="93.0" mnemonicParsing="false" onAction="#cancelClicked" prefHeight="25.0" prefWidth="54.0" text="Cancel" />
            <Button fx:id="okBtn" defaultButton="true" layoutX="371.0" layoutY="93.0" mnemonicParsing="false" onAction="#okClicked" prefHeight="25.0" prefWidth="54.0" text="Ok" />
            <TextArea fx:id="textArea" layoutX="30.0" layoutY="50.0" prefHeight="16.0" prefWidth="440.0" visible="false" />
         </children>
    </AnchorPane>
  </children>
</VBox>
