<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" prefHeight="57.0" prefWidth="561.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="filename" alignment="TOP_LEFT" layoutX="52.0" layoutY="9.0" prefHeight="42.0" prefWidth="250.0" text="Name of a file" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ProgressIndicator fx:id="progress" layoutX="396.0" layoutY="9.0" prefHeight="42.0" prefWidth="43.0" progress="0.0" />
            <Separator layoutX="311.0" layoutY="16.0" orientation="VERTICAL" prefHeight="28.0" prefWidth="3.0" />
            <Label fx:id="filesize" layoutX="328.0" layoutY="15.0" prefHeight="28.0" prefWidth="56.0" text="Filesize">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Separator layoutX="393.0" layoutY="15.0" orientation="VERTICAL" prefHeight="28.0" prefWidth="3.0" />
            <Separator layoutX="439.0" layoutY="15.0" orientation="VERTICAL" prefHeight="28.0" prefWidth="3.0" />
            <Button fx:id="startButton" layoutX="445.0" layoutY="16.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="30.0" text="Button" />
            <ImageView fx:id="image" fitHeight="28.0" fitWidth="30.0" layoutX="8.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="pauseButton" layoutX="481.0" layoutY="16.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="30.0" text="Button" />
            <Button fx:id="cancelButton" layoutX="517.0" layoutY="16.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="30.0" text="Button" />
         </children>
    </AnchorPane>
  </children>
</VBox>
