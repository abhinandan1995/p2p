            <Separator layoutX="20.0" layoutY="422.0" prefHeight="3.0" prefWidth="256.0" />
            <Label fx:id="statusLabel" layoutX="22.0" layoutY="458.0" prefHeight="25.0" prefWidth="71.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button layoutX="226.0" layoutY="108.0" mnemonicParsing="false" onAction="#outputDir" prefHeight="25.0" prefWidth="46.0" text="Browse">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Label layoutX="23.0" layoutY="342.0" prefHeight="25.0" prefWidth="38.0" text="Select:" />
         </children>
    </AnchorPane>
  </children>
</VBox>
