<VBox prefHeight="532.0" prefWidth="300.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p2pApp.p2pUi.controller.PreferencesDialogController">
  <children>
      <HBox prefHeight="32.0" prefWidth="298.0" style="-fx-background-color: #9dd2d3;">
