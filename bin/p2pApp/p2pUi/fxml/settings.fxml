            <TextField fx:id="outputFolder" editable="false" layoutX="97.0" layoutY="108.0" onKeyPressed="#notifyChanges3" prefHeight="25.0" prefWidth="129.0" promptText="d:/p2p/downloads/" />
            <Label layoutX="22.0" layoutY="57.0" prefHeight="25.0" prefWidth="71.0" text="Ip Address:" />
            <Label layoutX="22.0" layoutY="108.0" prefHeight="25.0" prefWidth="71.0" text="Save in: " />
            <TextField fx:id="inputPath" accessibleText="Hello" editable="false" layoutX="23.0" layoutY="385.0" prefHeight="25.0" prefWidth="191.0" promptText="Selected path will be shown here..." />
            <Label layoutX="22.0" layoutY="145.0" prefHeight="25.0" prefWidth="96.0" text="Shared Paths:" />
            <Label layoutX="91.0" layoutY="4.0" prefHeight="35.0" prefWidth="104.0" text=" Preferences" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="inputDepth" layoutX="190.0" layoutY="342.0" prefHeight="25.0" prefWidth="90.0" />
            <Label layoutX="22.0" layoutY="311.0" prefHeight="25.0" prefWidth="59.0" text="Add Path" />
            <Button fx:id="addPathBtn" layoutX="224.0" layoutY="385.0" mnemonicParsing="false" onAction="#addNewPath" prefHeight="25.0" prefWidth="51.0" text="Add " />
            <Button fx:id="saveBtn" defaultButton="true" layoutX="110.0" layoutY="453.0" mnemonicParsing="false" onAction="#saveChanges" prefHeight="35.0" prefWidth="78.0" text="Save">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Button>
            <Button fx:id="fileBtn" layoutX="68.0" layoutY="342.0" mnemonicParsing="false" onAction="#openFileList" prefHeight="25.0" prefWidth="51.0" text="File" />
            <Button fx:id="folderBtn" layoutX="124.0" layoutY="342.0" mnemonicParsing="false" onAction="#openDirList" prefHeight="25.0" prefWidth="59.0" text="Folder" />
